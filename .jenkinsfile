pipeline
{
	agent any

	stages
	{
withMaven(globalMavenSettingsConfig: '', jdk: 'JavaJDK', maven: 'maven', mavenSettingsConfig: '', traceability: true) {
   
      stage('Build')
      {
      steps
      {
        bat "mvn clean"
      }
      }
      stage('Deploy')
      {
      steps{
        bat "mvn install"
      }
      }

       stage('Test')
      {
      steps
      {
        bat "mvn test"
      }
      }

       stage('Release')
      {
      steps
      {
        echo 'releasing the project'
      }
      }


	}
}
}
